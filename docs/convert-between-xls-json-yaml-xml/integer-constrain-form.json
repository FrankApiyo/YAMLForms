{
  "type": "survey",
  "name": "integer-form",
  "title": "Integer Form",
  "id_string": "integer-form",
  "sms_keyword": "integer-form",
  "default_language": "default",
  "children": [
    {
      "name": "integer_question",
      "label": "Enter an Integer (<= 150)",
      "type": "integer",
      "required": true,
      "bind": {
        "constraint": ". <= 150",
        "jr:constraintMsg": "Value must be less than or equal to 150"
      }
    },
    {
      "name": "meta",
      "type": "group",
      "control": {
        "bodyless": true
      },
      "children": [
        {
          "name": "instanceID",
          "bind": {
            "readonly": "true()",
            "jr:preload": "uid"
          },
          "type": "calculate"
        }
      ]
    }
  ]
}
